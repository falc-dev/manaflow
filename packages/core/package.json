{
  "name": "@manaflow/core",
  "version": "0.1.0",
  "description": "Core game and replay engine for Manaflow",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@manaflow/types": "workspace:*",
    "js-yaml": "4.1.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  }
}
