{
  "name": "@manaflow/react",
  "version": "0.1.0",
  "description": "React wrappers for Manaflow replay viewer",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@manaflow/core": "workspace:*",
    "@manaflow/html-visor": "workspace:*",
    "@manaflow/types": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/react": "18.2.48",
    "react": "18.2.0",
    "typescript": "5.3.3",
    "vitest": "1.2.2"
  }
}
